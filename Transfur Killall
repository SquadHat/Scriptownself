for _,v in pairs(game:GetService"Players":GetPlayers()) do
local args = {
    [1] = v.Character.Humanoid,
    [2] = 100000,
    [3] = game:GetService"ReplicatedStorage"["Daddy~ -w-"]:FindFirstChildOfClass("StringValue").Value
}
game:GetService("ReplicatedStorage").BulletDamage:FireServer(unpack(args))
end
